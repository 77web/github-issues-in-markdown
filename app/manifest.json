{
  "name": "github issues on esa.io",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "github issues on esa.io",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.esa.io/posts/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ]
    }
  ]
}
